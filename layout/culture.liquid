    function isHomepage() {
      var p = window.location.pathname;
      return p === "/" || p === "/index.html";
    }
    function runIntroOncePerSession() {
      // &#128683; Never run anything on the password page
      if (isPasswordPage()) return;
      // &#9989; Only run on homepage
      if (!isHomepage()) return;
      var ix = (window.Webflow && Webflow.require) ? Webflow.require("ix3") : null;
      if (!ix) return; // Webflow not ready yet
      var isDesktop = window.innerWidth > 768;
      if (isDesktop) {
        if (!sessionStorage.getItem("introPlayed")) {
          ix.emit("introAnimation");        // play once on desktop
          sessionStorage.setItem("introPlayed", "true");
        } else {
          ix.emit("introAnimation_skip");   // jump to end after first time
        }
      } else {
        ix.emit("introAnimation_skip");     // always jump to end on mobile
      }
    }
    // Initial load
    document.addEventListener("DOMContentLoaded", runIntroOncePerSession);
    // If using Swup, also run after page swaps
    document.addEventListener("swup:contentReplaced", runIntroOncePerSession);
  })();
</script>

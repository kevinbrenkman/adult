  <script data-swup-ignore-script="">
(function () {
  var CONTAINER = '.main-wrapper';
  var FOOTER_SEL = 'footer.section_footer, footer[li-section="footer"]';
  var ioSticky = null;
  var lenis = window.lenis || null;
  // Start Lenis once (global) if not already
  if (!lenis && window.Lenis && (window.Webflow?.env("editor") === undefined)) {
    lenis = new Lenis({ lerp: 0.8, wheelMultiplier: 1, gestureOrientation: "vertical", normalizeWheel: false, smoothTouch: false });
    window.lenis = lenis;
    requestAnimationFrame(function raf(t){ lenis.raf(t); requestAnimationFrame(raf); });
  }
  function reinitWebflowIX(){
    try {
      if (window.Webflow?.destroy) window.Webflow.destroy();
      setTimeout(function(){
        var ix = (window.Webflow && (Webflow.require('ix2') || Webflow.require('ix3'))) || null;
        if (ix?.init) ix.init();
      }, 60);
    } catch(e){}
  }
  function underlineShopIfProduct(){
    var onProduct = /\/products\//.test(location.pathname);
    var links = Array.from(document.querySelectorAll('.main-nav_link-list .menu_link'));
    links.forEach(a=>a.classList.remove('is-current-shop'));
    if (!onProduct) return;
    var shopLink = links.find(a => /shop/i.test((a.textContent||'').trim()))
                 || links.find(a => (a.pathname||'').indexOf('/collections')>-1);
    if (shopLink) shopLink.classList.add('is-current-shop');
  }
  function scrollToAnchor(){
    var hash = location.hash;
    if (!hash) return;
    var target = document.querySelector(hash);
    if (!target) return;
    setTimeout(function(){
      if (window.lenis) window.lenis.scrollTo(target, { offset: 0 });
      else target.scrollIntoView({ behavior:'smooth' });
    }, 0);
  }
  function nudgeReflow(){
    var el = document.querySelector(CONTAINER);
    var ft = document.querySelector(FOOTER_SEL);
    if (el) el.getBoundingClientRect();
    if (ft) ft.getBoundingClientRect();
  }
  // --- Sticky product header (mount/unmount) ---
  function mountStickyHeader(){
    if (!/\/products\//.test(location.pathname)) return;
    var trigger = document.querySelector('.viewport-trigger');
    var stickyTarget = document.querySelector('.product-header_component');
    var mediaList = document.querySelector('.product-media-list');
    if (!trigger || !stickyTarget || !mediaList) return;
    if (stickyTarget.dataset.stickyInit === '1') return;
    stickyTarget.dataset.stickyInit = '1';
    ioSticky = new IntersectionObserver(function (entries) {
      var entry = entries[0];
      if (!entry) return;
      if (entry.isIntersecting) {
        stickyTarget.classList.add('sticky');
        mediaList.classList.add('remove-margin');
      } else if (entry.boundingClientRect.top > 0) {
        stickyTarget.classList.remove('sticky');
        mediaList.classList.remove('remove-margin');
      }
    }, { threshold: 0 });
    ioSticky.observe(trigger);
  }
  function unmountStickyHeader(){
    if (ioSticky) { try{ ioSticky.disconnect(); }catch(e){} ioSticky = null; }
    var el = document.querySelector('.product-header_component');
    if (el){ el.classList.remove('sticky'); delete el.dataset.stickyInit; }
    var ml = document.querySelector('.product-media-list');
    if (ml) ml.classList.remove('remove-margin');
  }
  // Misc small utilities that must reattach
  function initDynamicCurrentYear(){
    document.querySelectorAll('[data-current-year]').forEach(function(el){
      el.textContent = String(new Date().getFullYear());
    });
  }
  function initLoginModal(){
    const openTrigger = document.querySelector("#openLogin");
    const closeTrigger = document.querySelector("#closeLogin");
    const target = document.querySelector(".section_under-construction");
    if (!target) return;
    if (openTrigger) openTrigger.addEventListener("click", (e)=>{ e.preventDefault(); target.classList.add("open"); }, { once:true });
    if (closeTrigger) closeTrigger.addEventListener("click", (e)=>{ e.preventDefault(); target.classList.remove("open"); });
  }
  function initAboutCart(){
    const body = document.body;
    const mobileBg = document.querySelector('.mobile-menu-background');
    const mobileAboutBg = document.querySelector('.mobile-about-background');
    const cartList = document.querySelector('.nav_mini-cart-holder.w-dropdown-list');
    const aboutWrapper = document.querySelector('.about_component-wrapper');
    const aboutBtn = document.getElementById('aboutBtn');
    const closeBtn = document.getElementById('closeAbout');
    const modalBg = document.getElementById('bgcloseAbout');
    if (!cartList || !aboutWrapper) return;
    const cartRoot = cartList.closest('.w-dropdown') || cartList.parentElement;
    const cartToggle = cartRoot ? cartRoot.querySelector('.w-dropdown-toggle') : null;
    let y0 = 0;
    function lock(){ if(body.classList.contains('_lock')) return; y0 = window.scrollY||0; body.style.top= `-${y0}px`; body.classList.add('_lock'); }
    function unlock(){ if(!body.classList.contains('_lock')) return; body.classList.remove('_lock'); body.style.top=''; window.scrollTo(0, y0||0); }
    function isCartOpen(){ return cartList.classList.contains('w--open'); }
    function closeCart(){ cartList.classList.remove('w--open'); cartToggle?.classList.remove('w--open'); cartToggle?.setAttribute('aria-expanded','false'); }
    function openAbout(){ if (isCartOpen()) closeCart(); aboutWrapper.classList.add('open'); aboutBtn?.setAttribute('aria-expanded','true'); updateState(); }
    function closeAbout(){ aboutWrapper.classList.remove('open'); aboutBtn?.setAttribute('aria-expanded','false'); updateState(); }
    function toggleAbout(){ aboutWrapper.classList.contains('open') ? closeAbout() : openAbout(); }
    aboutBtn?.addEventListener('click', (e)=>{ e.preventDefault(); toggleAbout(); });
    closeBtn?.addEventListener('click',(e)=>{ e.preventDefault(); closeAbout(); });
    modalBg?.addEventListener('click', ()=> closeAbout());
    document.addEventListener('keydown', (e)=>{ if (e.key === 'Escape' && aboutWrapper.classList.contains('open')) closeAbout(); });
    const mo = new MutationObserver(updateState);
    mo.observe(cartList, { attributes: true, attributeFilter: ['class'] });
    function updateMobileBg(){
      if (!mobileBg) return;
      if (window.innerWidth < 767 && isCartOpen()) mobileBg.classList.add('open');
      else mobileBg.classList.remove('open');
    }
    function updateMobileAboutBg(){
      if (!mobileAboutBg) return;
      if (aboutWrapper.classList.contains('open')) mobileAboutBg.classList.add('open');
      else mobileAboutBg.classList.remove('open');
    }
    function updateState(){
      if (isCartOpen()) { aboutWrapper.classList.remove('open'); aboutBtn?.setAttribute('aria-expanded','false'); mobileAboutBg?.classList.remove('open'); }
      if (aboutWrapper.classList.contains('open') || isCartOpen()) lock(); else unlock();
      updateMobileBg(); updateMobileAboutBg();
    }
    window.addEventListener('resize', updateState, { passive:true });
    updateState();
  }
  function mount(){
    reinitWebflowIX();
    underlineShopIfProduct();
    initDynamicCurrentYear();
    initLoginModal();
    initAboutCart();
    scrollToAnchor();
  }
  // Boot swup (once)
  if (!window.swup) {
    window.swup = new Swup({
      containers: [CONTAINER],
      plugins: [
        new SwupScriptsPlugin({ head: false, body: true }),
        new SwupScrollPlugin({
          doScrollingRightAway: false,
          animateScroll: { betweenPages: false, samePageWithHash: false, samePage: true },
          offset: 0
        })
      ]
    });
    // Lifecycle
    swup.hooks.on('visit:start', function(){ unmountStickyHeader(); });
    swup.hooks.on('content:replace', nudgeReflow);
    swup.hooks.on('page:view', function(){
      mountStickyHeader();
      mount();
    });
  }
  // First paint
  document.readyState === 'loading'
    ? document.addEventListener('DOMContentLoaded', function(){ mountStickyHeader(); mount(); }, { once:true })
    : (mountStickyHeader(), mount());
})();
</script>
  <!--  Mini-cart plural fixer (Swup + Alpine safe)  -->
  <script>
/**
 * Mini-cart plural fixer
 * - Works with Alpine `@carttotalitems.window`
 * - Re-inits on Swup v4 navigations
 * - Cleans observers on re-injection
 */
(function () {
  if (window.__pluralFix && window.__pluralFix.teardown) window.__pluralFix.teardown();
  const state = { observers: [], pairs: [], handler: null, swup2: null, swup3: null };
  function getCount(el){ const m=(el.textContent||'').match(/\d+/); return m?parseInt(m[0],10):0; }
  function setPluralDisplay(pluralEl, hide){ if(!pluralEl) return; hide ? (pluralEl.style.display='none') : pluralEl.style.removeProperty('display'); }
  function updatePair(countEl, pluralEl){ setPluralDisplay(pluralEl, getCount(countEl) === 1); }
  function makePairs(root){
    const pairs = [];
    root.querySelectorAll('[li-element="mini-cart-item-count"]').forEach((countEl)=>{
      let pluralEl=null, sib=countEl.nextElementSibling;
      while(sib && !pluralEl){ pluralEl = sib.querySelector?.('.cart-plural')||null; sib = pluralEl?null:sib.nextElementSibling; }
      if(!pluralEl && countEl.parentElement){
        const plurals=[...countEl.parentElement.querySelectorAll('.cart-plural')];
        pluralEl = plurals.find(pl => (pl.compareDocumentPosition(countEl) & Node.DOCUMENT_POSITION_PRECEDING)!==0) || null;
      }
      if(pluralEl) pairs.push({countEl, pluralEl});
    });
    return pairs;
  }
  function observePair(countEl, pluralEl){
    const mo = new MutationObserver(()=>updatePair(countEl,pluralEl));
    mo.observe(countEl,{childList:true,characterData:true,subtree:true});
    state.observers.push(mo);
  }
  function disconnectAll(){ state.observers.forEach(o=>o.disconnect()); state.observers = []; }
  function init(root=document){
    disconnectAll();
    state.pairs = makePairs(root);
    state.pairs.forEach(({countEl,pluralEl})=>{ updatePair(countEl,pluralEl); observePair(countEl,pluralEl); });
    if (!state.handler) {
      state.handler = () => setTimeout(()=> state.pairs.forEach(({countEl,pluralEl})=>updatePair(countEl,pluralEl)), 0);
      window.addEventListener('carttotalitems', state.handler, { passive:true });
    }
  }
  function teardown(){
    disconnectAll();
    if (state.handler) window.removeEventListener('carttotalitems', state.handler);
    state.handler = null;
    if (state.swup2) document.removeEventListener('swup:contentReplaced', state.swup2);
    if (state.swup3?.off) state.swup3.off('page:view', init);
    state.swup2 = state.swup3 = null;
    state.pairs = [];
  }
  // expose for debugging
  window.__pluralFix = { refresh: ()=>init(document), teardown };
  // first run
  document.readyState === 'loading'
    ? document.addEventListener('DOMContentLoaded', ()=>init(document), { once:true })
    : init(document);
  // Swup v2/v4 support
  document.addEventListener('swup:contentReplaced', ()=>init(document)); // legacy
  if (window.swup?.hooks) {
    swup.hooks.on('page:view', ()=>init(document)); // v4
  }
})();
</script>
  <!--  Form success + scroll restore (optional, deduped)  -->
  <script>
(function(){
  function restoreScroll(anchor){
    const url = location.href;
    if (!url.includes(`?customer_posted=true#${anchor}`)) return;
    const y = sessionStorage.getItem('scrollPosition');
    if (y){ window.scrollTo(0, parseInt(y,10)); sessionStorage.removeItem('scrollPosition'); }
  }
  function handleSuccess(anchor, selectors){
    const url = location.href;
    if (!url.includes(`?customer_posted=true#${anchor}`)) return;
    const init = document.querySelector(selectors.initial);
    const ok   = document.querySelector(selectors.success);
    if (init) init.classList.add('succes');
    if (ok)   ok.classList.add('succes');
    // optional: show elements gated by [hs-newsletter-show]
    document.querySelectorAll('[hs-newsletter-show]').forEach(el=>{ el.style.display='flex'; });
  }
  function run(){
    restoreScroll('contact_form');
    handleSuccess('contact_form', { initial: '.footer-form-initial-state', success: '.footer-form-succes-state' });
    restoreScroll('wf-form-Contact-Form');
    handleSuccess('wf-form-Contact-Form', { initial: '.contact-form-wrapper', success: '.contact-form-succes-state' });
  }
  window.addEventListener('beforeunload', ()=> sessionStorage.setItem('scrollPosition', window.scrollY));
  document.readyState==='loading' ? document.addEventListener('DOMContentLoaded', run, { once:true }) : run();
  if (window.swup?.hooks) swup.hooks.on('page:view', run);
})();
</script>

  <script data-swup-ignore-script="">
  const swup = new Swup({
    plugins: [
      new SwupScriptsPlugin({ head: true, body: true }),
      new SwupScrollPlugin({
        doScrollingRightAway: false,
        animateScroll: {
          betweenPages: false,
          samePageWithHash: false,
          samePage: true
        },
        scrollFriction: 0.3,
        scrollAcceleration: 0.04,
        scrollContainers: `[data-swup-scroll-container]`,
        offset: 0,
        markScrollTarget: false,
        getAnchorElement: null,
        shouldResetScrollPosition: () => true
      })
    ]
  });
  function reinitWebflow() {
    if (window.Webflow && typeof window.Webflow.destroy === "function") {
      window.Webflow.destroy();
    }
    setTimeout(() => {
      if (window.Webflow && typeof window.Webflow.require === "function") {
        window.Webflow.require("ix2").init();
      }
    }, 100);
  }
  function scrollToAnchor() {
    const hash = window.location.hash;
    if (hash && document.querySelector(hash)) {
      const target = document.querySelector(hash);
      setTimeout(() => {
        if (window.lenis) {
          window.lenis.scrollTo(target, { offset: 0 });
        } else {
          target.scrollIntoView({ behavior: 'smooth' });
        }
      }, 0); // Let Webflow finish layout shift
    }
  }
  // Force scroll position reset BEFORE transition starts (important!)
  swup.hooks.on('visit:start', () => {
    window.scrollTo(0, 0);
  });
  // After new page is rendered
  swup.hooks.on('page:view', () => {
    reinitWebflow();
    scrollToAnchor();
  });
  // On initial page load too
  document.addEventListener('DOMContentLoaded', () => {
    scrollToAnchor();
  });
</script>

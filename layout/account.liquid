  <script>
document.querySelectorAll('.modal-button').forEach(button => {
  button.addEventListener('click', () => {
    const plus = button.querySelector('.plus');
    const modal = button.nextElementSibling;
    if (!plus || !modal || !modal.classList.contains('modal')) return;
    const isOpen = modal.classList.contains('open');
    // Toggle +/-
    plus.textContent = isOpen ? '+' : '-';
    // Toggle .open class
    modal.classList.toggle('open');
    // Remove leftover inline styles if closing
    if (isOpen) {
      modal.removeAttribute('style');
    }
  });
});
</script>
  <script>
  document.addEventListener('DOMContentLoaded', () => {
    const countEl = document.querySelector('[li-element="mini-cart-item-count"]');
    const pluralEl = document.querySelector('.cart-plural');
    function updatePlural() {
      if (!countEl || !pluralEl) return;
      const count = parseInt(countEl.textContent.trim(), 10);
      pluralEl.style.display = count === 1 ? 'none' : '';
    }
    updatePlural();
    // If Liquify updates the count dynamically, use MutationObserver
    const observer = new MutationObserver(updatePlural);
    if (countEl) {
      observer.observe(countEl, { childList: true, characterData: true, subtree: true });
    }
  });
</script>
  <script>
 document.addEventListener("DOMContentLoaded", function () {
  const trigger = document.querySelector(".viewport-trigger");
  const stickyTarget = document.querySelector(".product-header_component");
  const mediaList = document.querySelector(".product-media-list");
  if (!trigger || !stickyTarget || !mediaList) return;
  const observer = new IntersectionObserver(
    ([entry]) => {
      if (entry.isIntersecting) {
        stickyTarget.classList.add("sticky");
        mediaList.classList.add("remove-margin");
      } else if (entry.boundingClientRect.top > 0) {
        stickyTarget.classList.remove("sticky");
        mediaList.classList.remove("remove-margin");
      }
    },
    {
      threshold: 0,
    }
  );
  observer.observe(trigger);
});
</script>

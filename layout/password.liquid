  document.addEventListener('swup:contentReplaced', ()=>init(document)); // legacy
  if (window.swup?.hooks) {
    swup.hooks.on('page:view', ()=>init(document)); // v4
  }
})();
</script>
  <!--  Form success + scroll restore (optional, deduped)  -->
  <script>
(function(){
  function restoreScroll(anchor){
    const url = location.href;
    if (!url.includes(`?customer_posted=true#${anchor}`)) return;
    const y = sessionStorage.getItem('scrollPosition');
    if (y){ window.scrollTo(0, parseInt(y,10)); sessionStorage.removeItem('scrollPosition'); }
  }
  function handleSuccess(anchor, selectors){
    const url = location.href;
    if (!url.includes(`?customer_posted=true#${anchor}`)) return;
    const init = document.querySelector(selectors.initial);
    const ok   = document.querySelector(selectors.success);
    if (init) init.classList.add('succes');
    if (ok)   ok.classList.add('succes');
    // optional: show elements gated by [hs-newsletter-show]
    document.querySelectorAll('[hs-newsletter-show]').forEach(el=>{ el.style.display='flex'; });
  }
  function run(){
    restoreScroll('contact_form');
    handleSuccess('contact_form', { initial: '.footer-form-initial-state', success: '.footer-form-succes-state' });
    restoreScroll('wf-form-Contact-Form');
    handleSuccess('wf-form-Contact-Form', { initial: '.contact-form-wrapper', success: '.contact-form-succes-state' });
  }
  window.addEventListener('beforeunload', ()=> sessionStorage.setItem('scrollPosition', window.scrollY));
  document.readyState==='loading' ? document.addEventListener('DOMContentLoaded', run, { once:true }) : run();
  if (window.swup?.hooks) swup.hooks.on('page:view', run);
})();
</script>
</body>

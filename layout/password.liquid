      if (isCartOpen()) { aboutWrapper.classList.remove('open'); aboutBtn?.setAttribute('aria-expanded','false'); mobileAboutBg?.classList.remove('open'); }
      if (aboutWrapper.classList.contains('open') || isCartOpen()) lock(); else unlock();
